                        <h2>Register</h2>

                        <form class="woocommerce-form woocommerce-form-login login"  action="{{ path("fos_user_registration_register") }}" method="post">


                            <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                                {{ form_row(form._token) }}

                                <label for="email">Email &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.email, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Email' } }) }}
                                {{ form_errors(form.email) }}

                                <label for="username">Username &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.username, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Username' } }) }}
                                {{ form_errors(form.username) }}

                                <label for="adress">Adress &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.adress, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Adress' } }) }}
                                {{ form_errors(form.adress) }}

                                <label for="Password">Password &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.plainPassword.first, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Password' } }) }}
                                {{ form_errors(form.plainPassword.first) }}

                                <label for="Password comfirm">Password comfirm &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.plainPassword.second, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Password comfirm' } }) }}
                                {{ form_errors(form.plainPassword.second) }}

                                <label for="phone number">Phone number &nbsp;<span class="required">*</span></label>
                                {{ form_widget(form.mobileNum, { 'attr': {'class': 'woocommerce-Input woocommerce-Input--text input-text', 'placeholder': 'Phone number' } }) }}
                                {{ form_errors(form.mobileNum) }}

                            </p>
                            <p class="form-row">
                              <button type="submit" class="woocommerce-Button button" name="register" value="register">register</button>
                            </p>



                        </form>


